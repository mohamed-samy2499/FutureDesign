<style>
    .SearchBar
    {
        display:flex;
        justify-content: center;
    }
    .home{
        background-color: #FAFAFA;
    }
    .home .container
{
    padding-top: 50px;
    padding-bottom: 50px;
}
.logo-bar
{
    position: fixed;
    z-index: 99999;
    left: 0px;
    top: 20%;
    bottom: 70%;
    background-color:transparent /*#f8f6f6*/;
    width: 50px;

}
.logo-bar i
{
    transition: 0.5s;
}
.logo-bar i:hover
{   
    cursor: pointer;
    color: #f25454;
}
.toggle-button
{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.toggle-button i
{
    font-weight: 900;
    font-size: 25px;

}
.options-bar
{
    position: fixed;
    z-index: 999999;
    left: -50px;
    top: 15%;
    bottom: 65%;
    background-color:transparent /*#f8f6f6*/;
    width: 50px;

}
.options-bar ul
{
    
    padding-left: 20px;
    height: 300px;
    list-style-type: none;
}
.options-bar li
{
    padding-top: 17px;
}
.options-bar i
{
    font-size: 25px;
}
.options-bar li
{
    text-decoration: none;
    transition: all 0.5s;
}
.options-bar a
{
    text-decoration: none;
    transition: all 0.5s;
}
.options-bar li:hover{

    color: #f25454;
}


.awl-li
{
    transform: translateY(1000px);
    cursor: pointer;
    transition: all 0.5s;
    transition-duration: 2s;
    opacity: 0;
}
.tany-li
{
    transform: translateY(1300px);
    cursor: pointer;
    transition: all 0.5s;
    transition-duration: 1.8s;
    opacity: 0;
}
.talt-li
{
    transform: translateY(1600px);
    cursor: pointer;
    transition: all 0.5s;
    transition-duration: 1.6s;
    opacity: 0;
}


.awl1-li
{
    /*transform: translateY(1000px);*/
    cursor: pointer;
    transition: all 0.5s;
    transition-duration: 1s;
    opacity: 1;
}
.tany1-li
{
    /*transform: translateY(1300px);*/
    cursor: pointer;
    transition: all 0.5s;
    transition-duration: 1.2s;
    opacity: 1;
}
.talt1-li
